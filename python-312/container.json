{
  "name": "Python Dev Container",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "image": "jscontainer:latest",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    },
    "jetbrains": {
      "projector": {
        "ide": "PyCharm",
        "version": "2024.1"
      }
    }
  },
  "settings": {
    "python.pythonPath": "/usr/local/bin/python"
  },
  "remoteUser": "jsuser",
  "mounts": [
    "source=E:\\Python\\efetl,target=/workspace/efetl,type=bind,consistency=cached",
    "source=E:\\Python\\jetshift-core,target=/workspace/jetshift-core,type=bind,consistency=cached"
  ],
  "workspaceFolders": [
    "/workspace/efetl",
    "/workspace/jetshift-core"
  ]
}
